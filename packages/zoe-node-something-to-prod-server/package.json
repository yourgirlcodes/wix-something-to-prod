{
  "name": "@wix/zoe-node-something-to-prod-server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": {
    "name": "zoe cohen",
    "email": "zoec@wix.com"
  },
  "main": "dist/src/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run proto && npm run tsc",
    "clean": "rm -rf dist target src/generated",
    "lint": "eslint --resolve-plugins-relative-to node_modules/@wix/wnp-shared-config --fix --ext .js,.ts .",
    "proto": "DEBUG=wix:* wix-proto-codegen --module . --dir src/generated",
    "test": "jest --runInBand --forceExit",
    "posttest": "npm run lint -s",
    "tsc": "tsc"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testRegex": ".*\\.(spec|it)\\.(ts|js)",
    "testTimeout": 20000
  },
  "dependencies": {
    "@wix/wix-aspects": "~1.0.640",
    "@wix/wix-bootstrap-api-gw-client": "~1.0.0",
    "@wix/wix-bootstrap-ng": "~1.0.1808",
    "acorn-jsx": "^5.3.2",
    "lint": "^0.7.0",
    "protobuf-fieldmask": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "~27.4.0",
    "@types/uuid": "~8.3.4",
    "@wix/wix-api-gw-lite-testkit": "~1.0.0",
    "@wix/wix-bootstrap-testkit": "~1.1.866",
    "@wix/wix-proto-codegen": "~2.0.0",
    "@wix/wix-test-env": "~1.0.0",
    "@wix/wnp-shared-config": "~1.0.596",
    "@wix/zoe-node-something-to-prod-api": "~1.0.0",
    "eslint": "~7.32.0",
    "eslint-plugin-jest": "~24.5.0",
    "jest": "~27.4.7",
    "ts-jest": "~27.1.3",
    "typescript": "~4.5.5",
    "uuid": "~8.3.2"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/",
    "versionBumpStrategy": "patch"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.examples",
      "artifactId": "zoe-node-something-to-prod-server",
      "targets": {
        "static": false,
        "docker": true
      }
    }
  }
}
