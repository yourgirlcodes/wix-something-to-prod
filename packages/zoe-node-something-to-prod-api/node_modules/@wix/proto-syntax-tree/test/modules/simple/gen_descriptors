#!/bin/bash
protoc -I ../../../include google/protobuf/wrappers.proto -o fds/wrappers.bin  --include_source_info
protoc -I ../../../include google/protobuf/descriptor.proto -o fds/descriptor.bin  --include_source_info
protoc -I ../../../include google/api/http.proto -o fds/http.bin  --include_source_info
protoc -I ../../../include google/api/annotations.proto -o fds/annotations.bin  --include_source_info
protoc ../include/com/include/messages.proto -I../include -o fds/include.bin  --descriptor_set_in=fds/wrappers.bin:fds/http.bin:fds/annotations.bin:fds/descriptor.bin --include_source_info

protoc com/wix/service.proto com/wix/messages.proto -I. -o fds/main.bin  --descriptor_set_in=fds/wrappers.bin:fds/http.bin:fds/annotations.bin:fds/descriptor.bin:fds/include.bin --include_source_info
