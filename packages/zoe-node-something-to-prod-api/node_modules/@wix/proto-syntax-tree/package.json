{
  "name": "@wix/proto-syntax-tree",
  "version": "1.0.229",
  "description": "protobuf syntax tree",
  "main": "dist/index.js",
  "author": "Eyal Post <eyalp@wix.com>",
  "license": "ISC",
  "scripts": {
    "proto": "protoc -I descriptor/proto mini_descriptor.proto --js_out=import_style=commonjs_strict:lib/descriptor-parser",
    "clean": "rm -rf ./dist ./lib/generated",
    "build:ts": "tsc -p tsconfig.build.json",
    "compile": "tsc",
    "build": "yarn run clean && yarn run build:ts && cp -r ./include ./dist/include && cp ./lib/descriptor-parser/*.js ./dist/lib/descriptor-parser/",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "yarn run run-tests && yarn run lint:fix",
    "run-tests": "jest"
  },
  "dependencies": {
    "@wix/extensions": "^1.0.687",
    "@wix/flynt-protoc": "1.0.16",
    "debug": "^4.3.4",
    "google-protobuf": "3.19.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/tmp": "^0.2.3",
    "tmp": "^0.2.1"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "proto-syntax-tree"
    },
    "validations": {
      "postBuild": [
        "run-tests",
        "lint"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/flynt.git",
    "directory": "packages/syntax-tree"
  }
}