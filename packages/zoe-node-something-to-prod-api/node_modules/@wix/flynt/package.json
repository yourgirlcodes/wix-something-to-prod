{
  "name": "@wix/flynt",
  "version": "1.0.498",
  "description": "wix protobuf linter",
  "main": "dist/index.js",
  "author": "Eyal Post <eyalp@wix.com>",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf ./dist ./bin",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run build:ts",
    "compile": "tsc",
    "cli": "ts-node cli.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "yarn run run-tests && yarn run-js-tests && yarn run lint:fix",
    "run-tests": "jest",
    "run-js-tests": "jest --config=test/js/jest.config.js"
  },
  "bin": {
    "flynt": "dist/cli.js"
  },
  "dependencies": {
    "@sentry/node": "^6.19.7",
    "@wix/flynt-engine": "1.0.278",
    "@wix/flynt-plugin-types": "1.0.12",
    "@wix/flynt-plugins-host": "1.0.19",
    "@wix/flynt-rules": "1.0.457",
    "@wix/proto-syntax-tree": "1.0.229",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/tmp": "^0.2.3",
    "@wix/flynt-protoc": "^1.0.0",
    "tmp": "^0.2.1"
  },
  "engines": {
    "node": ">=14"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "flynt"
    },
    "validations": {
      "postBuild": [
        "run-tests",
        "run-js-tests",
        "lint"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/flynt.git",
    "directory": "packages/cli"
  }
}