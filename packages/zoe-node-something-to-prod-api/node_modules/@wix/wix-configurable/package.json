{
  "name": "@wix/wix-configurable",
  "version": "1.0.903",
  "description": "module that copies over config templates for modules using it",
  "main": "index.js",
  "bin": {
    "copy-config-templates": "./scripts/copy-config-templates.js"
  },
  "author": "Vilius Lukosius",
