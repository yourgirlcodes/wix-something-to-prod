{
  "name": "@wix/wnp-protobufjs",
  "version": "1.0.312",
  "description": "wix protobufjs loader",
  "main": "index.js",
  "homepage": "https://github.com/wix-private/wix-node-platform/tree/master/rpc/wnp-protobufjs",
  "author": "Daniel Shmuglin <daniels@wix.com>",
  "license": "ISC",
  "scripts": {
    "build": ":",
    "lint": "eslint --resolve-plugins-relative-to node_modules/@wix/wnp-shared-config --fix --ext .js,.ts .",
    "test:mocha": "DEBUG='wix:*' mocha 'test/**/*.spec.js' 'test/**/*.it.js' 'test/**/*.it.ts'",
    "test:jest": "DEBUG='wix:*' jest --runInBand",
    "test": "npm run test:mocha && npm run test:jest",
    "posttest": "npm run lint -s",
    "patch:prepare": "rm -rf node_modules protobufjs && npm i --package-lock && npx patch-package && ln -s node_modules/protobufjs ./protobufjs",
    "patch:finish": "unlink ./protobufjs && rm -rf patches && npx patch-package protobufjs && cp -r node_modules/protobufjs . && rm -rf package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/wix-node-platform.git",
    "directory": "rpc/wnp-protobufjs"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "keywords": [
    "grpc",
    "rpc"
  ],
  "bugs": {
    "url": "https://github.com/wix-platform/wix-node-platform/issues"
  },
  "dependencies": {
    "protobufjs": "6.11.2"
  },
  "devDependencies": {
    "@wix/wnp-shared-config": "~1.0.596",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "jest": "27.4.7",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "long": "4.0.0",
    "mocha": "9.2.0",
    "mocha-env-reporter": "4.0.0",
    "patch-package": "6.4.7"
  },
  "jest": {
    "testRegex": "/__tests__/.*spec.js"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "wnp-protobufjs",
      "targets": {
        "static": false,
        "docker": false
      }
    }
  }
}