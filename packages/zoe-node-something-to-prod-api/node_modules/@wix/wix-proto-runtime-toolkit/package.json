{
  "name": "@wix/wix-proto-runtime-toolkit",
  "version": "1.0.645",
  "description": "JS/TS code generator runtime toolkit",
  "main": "index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/wix-private/wix-node-platform/tree/master/rpc/wix-proto-runtime-toolkit",
  "author": "Daniel Shmuglin <daniels@wix.com>",
  "license": " ISC",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run proto",
    "proto": "npm run proto:local && npm run proto:string-longs",
    "proto:local": "DEBUG=wix:* ./proto-codegen.sh --module ./test/proto-module --dir ../../target --filename local --tsClasses",
    "proto:string-longs": "DEBUG=wix:* ./proto-codegen.sh --module ./test/proto-module --dir ../../target --filename string-longs --stringLongs --tsClasses",
    "lint": "eslint --resolve-plugins-relative-to node_modules/@wix/wnp-shared-config --fix --ext .js,.ts .",
    "test": "DEBUG='wix:*' mocha 'test/**/*.{spec,it}.{js,ts}'",
    "posttest": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/wix-node-platform.git",
    "directory": "rpc/wix-proto-runtime-toolkit"
  },
  "bugs": {
    "url": "https://github.com/wix-platform/wix-node-platform/issues"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "dependencies": {
    "@types/long": "4.0.1",
    "@wix/wnp-protobufjs": "1.0.312",
    "long": "4.0.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/chai-things": "0.0.35",
    "@types/mocha": "9.1.0",
    "@types/node": "14.14.41",
    "@wix/wix-aspects": "~1.0.640",
    "@wix/wnp-shared-config": "~1.0.596",
    "chai": "4.3.4",
    "chai-things": "0.2.0",
    "eslint": "7.32.0",
    "mocha": "9.2.0",
    "mocha-env-reporter": "4.0.0",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "wix-proto-runtime-toolkit",
      "targets": {
        "static": false,
        "docker": false
      }
    }
  }
}