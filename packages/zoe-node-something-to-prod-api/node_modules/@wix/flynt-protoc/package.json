{
  "name": "@wix/flynt-protoc",
  "version": "1.0.16",
  "description": "flynt protoc wrapper",
  "main": "dist/index.js",
  "author": "Eyal Post <eyalp@wix.com>",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "yarn run build:ts && cp -r ./lib/bin ./dist/lib/bin",
    "compile": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "yarn run run-tests && yarn run lint:fix",
    "run-tests": "jest"
  },
  "bin": {
    "protoc": "./cli.js"
  },
  "dependencies": {
    "is-ci": "^3.0.1",
    "debug": "^4.3.4",
    "glob": "^7.2.3",
    "tmp": "^0.2.1",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "@types/tmp": "^0.2.3"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "flynt-protoc"
    },
    "validations": {
      "postBuild": [
        "run-tests",
        "lint"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/flynt.git",
    "directory": "packages/protoc"
  }
}