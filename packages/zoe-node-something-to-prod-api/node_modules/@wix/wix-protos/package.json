{
  "name": "@wix/wix-protos",
  "version": "1.0.95",
  "description": "typed wix proto extensions",
  "author": "Eyal Post <eyalp@wix.com>",
  "license": "ISC",
  "main": "typed/index.js",
  "files": [
    "typed"
  ],
  "scripts": {
    "clean": "rm -rf ./typed ./src/fds ./src/google ./src/wix ./src/google.ts ./src/wix.ts",
    "gen-pre-build": "yarn run typed-options && yarn run descriptors",
    "compile": "yarn build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "yarn run clean && yarn run gen-pre-build && yarn build:ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "yarn run run-tests && yarn run lint:fix",
    "run-tests": "jest",
    "typed-options-google": "typed-proto-options --mainProtoFolder ../syntax-tree/include -O src && rm -rf ./src/google/protobuf",
    "typed-options-wix": "typed-proto-options --mainProtoFolder ../../node_modules/@wix/extensions/src/main/proto -I ../syntax-tree/include -O src",
    "typed-options": "yarn typed-options-wix && yarn typed-options-google",
    "common-descriptors": "protoc $(find ../../node_modules/@wix/protos-common/src/main/proto/wix/common/ -name \"*.proto\") $(find ../../node_modules/@wix/protos-query/src/main/proto/wix/common/ -name \"*.proto\") -o src/fds/protos-common.bin -I ../../node_modules/@wix/protos-query/src/main/proto -I ../../node_modules/@wix/protos-common/src/main/proto -I ../syntax-tree/include -I ../../node_modules/@wix/extensions/src/main/proto --include_imports",
    "fds-to-source": "ts-node -T scripts/fds-to-src.ts",
    "descriptors": "mkdir -p src/fds; yarn common-descriptors && yarn fds-to-source"
  },
  "dependencies": {
    "@wix/proto-syntax-tree": "1.0.229"
  },
  "devDependencies": {
    "@wix/flynt-protoc": "^1.0.0",
    "@wix/protos-common": "^1.0.264",
    "@wix/protos-query": "^1.0.237",
    "@wix/typed-proto-options": "^1.0.0"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "wix-protos"
    },
    "validations": {
      "postBuild": [
        "run-tests",
        "lint"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/flynt.git",
    "directory": "packages/wix-protos"
  }
}