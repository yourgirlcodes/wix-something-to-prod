{
  "name": "@wix/wnp-proto-validation-fv",
  "version": "1.0.83",
  "description": "wix proto validations using fast-validator",
  "main": "index.js",
  "homepage": "https://github.com/wix-private/wix-node-platform/tree/master/rpc/wnp-proto-validation-fv",
  "author": "Daniel Shmuglin <daniels@wix.com>",
  "license": "ISC",
  "scripts": {
    "build": "npm run clean && npm run proto",
    "clean": "rm -rf target",
    "lint": "eslint --resolve-plugins-relative-to node_modules/@wix/wnp-shared-config --fix --ext .js,.ts .",
    "proto:numeric-enums": "DEBUG=wix:* npx wix-proto-codegen --module ./test/proto-module --dir ../../target/dist/numeric --tsClasses --skipOptionsValidation",
    "proto:string-enums-and-longs": "DEBUG=wix:* npx wix-proto-codegen --module ./test/proto-module --dir ../../target/dist/string --tsClasses --stringEnums --stringLongs --skipOptionsValidation",
    "proto": "npm run proto:numeric-enums && npm run proto:string-enums-and-longs",
    "test": "DEBUG='wix:*' mocha 'test/**/*.spec.{js,ts}' 'test/**/*.it.{js,ts}'",
    "posttest": "npm run lint -s"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/wix-node-platform.git",
    "directory": "rpc/wnp-proto-validation-fv"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "keywords": [
    "fast-validator",
    "proto",
    "validation"
  ],
  "bugs": {
    "url": "https://github.com/wix-platform/wix-node-platform/issues"
  },
  "dependencies": {
    "@wix/locale-dataset-javascript": "1.0.3026",
    "@wix/wix-aspects": "1.0.1438",
    "@wix/wix-js-validations": "1.7.0",
    "@wix/wix-proto-runtime-toolkit": "1.0.645",
    "@wix/wnp-proto-runtime-types": "1.0.59",
    "@wix/wnp-protobufjs": "1.0.312",
    "@wix/wnp-rpc-common": "1.0.545",
    "bcp-47": "1.0.8",
    "bignumber.js": "9.0.0",
    "country-data": "0.0.31",
    "fastest-validator": "1.12.0",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.21",
    "long": "4.0.0",
    "node-uri": "1.1.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/long": "4.0.1",
    "@types/mocha": "9.1.0",
    "@wix/wnp-shared-config": "~1.0.596",
    "chai": "4.3.4",
    "eslint": "7.32.0",
    "long": "4.0.0",
    "mocha": "9.2.0",
    "mocha-env-reporter": "4.0.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "wnp-proto-validation-fv",
      "targets": {
        "static": false,
        "docker": false
      }
    }
  }
}