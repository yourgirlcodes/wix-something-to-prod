{
  "name": "@wix/flynt-engine",
  "version": "1.0.278",
  "description": "protobuf linter",
  "main": "dist/index.js",
  "author": "Eyal Post <eyalp@wix.com>",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf ./dist && yarn clean-gen",
    "clean-gen": "rm -f ./test/catalogs/valid-catalog/gen-index.ts && rm -f ./test/catalogs/test-catalog/gen-index.ts",
    "build:ts": "tsc -p tsconfig.build.json",
    "compile": "yarn gen-catalogs && tsc",
    "build": "yarn run clean && yarn run build:ts",
    "gen-catalogs": "concurrently 'yarn gen-catalog-1' 'yarn gen-catalog-2'",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "yarn run-tests && yarn run lint:fix",
    "run-tests": "yarn gen-catalogs && jest",
    "gen-catalog-1": "ts-node -T scripts/build-catalog-loader.ts ./test/catalogs/valid-catalog",
    "gen-catalog-2": "ts-node -T scripts/build-catalog-loader.ts ./test/catalogs/test-catalog"
  },
  "dependencies": {
    "@wix/proto-syntax-tree": "1.0.229",
    "debug": "^4.3.4",
    "glob": "^7.2.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/glob": "^7.2.0",
    "@types/valid-url": "^1.0.3",
    "concurrently": "^7.2.1",
    "handlebars": "^4.7.7",
    "type-fest": "^2.13.0"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress.npm",
      "artifactId": "flynt-engine"
    },
    "validations": {
      "postBuild": [
        "run-tests",
        "lint"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix-private/flynt.git",
    "directory": "packages/engine"
  }
}